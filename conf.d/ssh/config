# github
Host github.com
    User git
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github_rsa
    ServerAliveInterval 300
    ServerAliveCountMax 10

# gitlab
Host gitlab.com
    User git
    HostName gitlab.company.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitlab_rsa
    ServerAliveInterval 300
    ServerAliveCountMax 10

# huggingface
Host hf.co
	User git
    HostName hf.co
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/huggingface_rsa
    ServerAliveInterval 300
    ServerAliveCountMax 10
